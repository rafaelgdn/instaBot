{
  "name": "instabot",
  "version": "1.0.0",
  "description": "default repository to work like a guide to new micro-services",
  "author": "Rafael",
  "license": "ISC",
  "private": true,
  "scripts": {
    "deploy-dev": "sls deploy -s dev --verbose",
    "deploy-staging": "sls deploy -s staging --verbose",
    "deploy-production": "sls deploy -s production --verbose",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "serverless-bundle test --silent --coverage --noStackTrace --runInBand --passWithNoTests",
    "test:verbose": "serverless-bundle test --verbose --noStackTrace --runInBand --passWithNoTests",
    "test:coverage": "serverless-bundle test --coverage --noStackTrace --runInBand --passWithNoTests"
  },
  "dependencies": {
    "aws-sdk": "^2.1061.0",
    "imap-simple": "^5.1.0",
    "inquirer": "^8.2.4",
    "instagram-private-api": "^1.45.3",
    "lodash": "^4.17.21",
    "mailparser": "^3.5.0",
    "re2": "^1.17.2",
    "tough-cookie-filestore2": "^1.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "lint-staged": "^12.3.1",
    "prettier": "^2.5.1",
    "prettier-eslint": "^8.2.2",
    "serverless-bundle": "5.2.0",
    "serverless-dotenv-plugin": "^4.0.1",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-prune-plugin": "^2.0.1"
  }
